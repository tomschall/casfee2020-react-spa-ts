  "scripts": {
    "dev:start": "yarn run api:hasura:migrations:apply && npm run api:client:generate && yarn start",
    "api:hasura:migrations:apply": "hasura --envfile ../.env --project hasura migrate apply && hasura --envfile ../.env --project hasura metadata apply",
    "api:hasura:console": "hasura --envfile ../.env --project hasura console",
    "api:client:generate": "DEBUG=true rimraf src/api/generated/* && graphql-codegen --config codegen.js",
    "start": "react-scripts start",
    "build": "npm run api:client:generate && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.17.8",
    "@graphql-codegen/introspection": "^1.17.8",
