  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-common": "^3.1.4",
    "@apollo/client": "^3.3.9",
    "@apollo/react-hooks": "^4.0.0",
    "@auth0/auth0-react": "^1.2.0",
    "@giphy/js-fetch-api": "^2.1.0",
    "@giphy/react-components": "^2.1.3",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "@types/jest": "^24.0.0",
    "@types/node": "14.14.25",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "@types/uuid": "^8.3.0",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.11.0",
    "graphql-tools": "^6.0.16",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.20",
    "material-ui-popup-state": "^1.7.1",
    "moment": "^2.29.1",
    "nightwatch": "^1.5.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-resize-observer": "^1.1.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.4",
    "recoil": "^0.1.2",
    "subscriptions-transport-ws": "^0.9.18",
    "typescript": "^3.9.7",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "dev:start": "yarn run api:hasura:migrations:apply && yarn api:client:generate && yarn start",
    "dev:start:windows": "yarn run api:hasura:migrations:apply && yarn api:client:generate:windows && yarn start",
    "api:hasura:migrations:apply": "hasura --envfile ../.env --project hasura migrate apply && hasura --envfile ../.env --project hasura metadata apply",
    "api:hasura:console": "hasura --envfile ../.env --project hasura console",
    "api:client:generate": "DEBUG=true rimraf src/api/generated/* && graphql-codegen --config codegen.js",
    "api:client:generate:windows": "rimraf src/api/generated/* && graphql-codegen --config codegen.js",
    "start": "react-scripts start",
    "build": "yarn api:client:generate && sh -ac '. .env.build; react-scripts build'",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "nightwatch": "npx nightwatch",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.20.1",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/typescript": "^1.20.2",
    "@graphql-codegen/typescript-graphql-request": "^2.0.0",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@graphql-codegen/typescript-react-apollo": "^2.0.6",
    "chromedriver": "^86.0.0",
    "geckodriver": "^1.22.1",
    "jest-environment-jsdom-sixteen": "^1.0.3"
  }
}
